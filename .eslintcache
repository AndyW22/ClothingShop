h = null;\n\n  componentDidMount() {\n    this.unsubscribeFromAuth = auth.onAuthStateChanged(async (userAuth) => {\n      if (userAuth) {\n        const userRef = await createUserProfileDocument(userAuth);\n        userRef.onSnapshot((snapShot) => {\n          this.setState({\n            currentUser: { id: snapShot.id, ...snapShot.data() },\n          });\n        });\n      } else {\n        this.setState({ currentUser: userAuth });\n      }\n    });\n  }\n\n  componentWillUnmount() {\n    this.unsubscribeFromAuth();\n  }\n\n  render() {\n    return (\n      <div>\n        <Header currentUser={this.state.currentUser} />\n        <button onClick={() => console.log(this.state.currentUser)}>\n          Click me\n        </button>\n        <Switch>\n          <Route exact path=\"/\" component={Homepage} />\n          <Route exact path=\"/shop\" component={ShopPage} />\n          <Route path=\"/signin\" component={SignInPage} />\n        </Switch>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/andywaller/Documents/Coding/masterproject/clothingshop/src/pages/homepage/homepage.component.jsx",[],"/Users/andywaller/Documents/Coding/masterproject/clothingshop/src/components/directory/directory.component.jsx",[],"/Users/andywaller/Documents/Coding/masterproject/clothingshop/src/components/menu-item/menu-item.component.jsx",[],"/Users/andywaller/Documents/Coding/masterproject/clothingshop/src/pages/shop/shop.component.jsx",[],"/Users/andywaller/Documents/Coding/masterproject/clothingshop/src/pages/shop/shop.data.js",[],"/Users/andywaller/Documents/Coding/masterproject/clothingshop/src/components/preview-collection/preview-collection.component.jsx",[],"/Users/andywaller/Documents/Coding/masterproject/clothingshop/src/components/collection-item/collection-item.component.jsx",[],["78","79","80","81","82"],"/Users/andywaller/Documents/Coding/masterproject/clothingshop/src/components/header/header.component.jsx",[],"/Users/andywaller/Documents/Coding/masterproject